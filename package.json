{
  "name": "react-leaflet",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --port $PORT",
    "docker:dev": "docker compose run --service-ports react-leaflet bash",
    "build": "tsc && vite build",
    "lint": "eslint map-sector-creator --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "prettier . --write"
  },
  "dependencies": {
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@apollo/client": "^3.8.1",
    "@types/leaflet": "^1.9.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "graphql": "^16.8.0",
    "leaflet": "^1.9.4",
    "react-leaflet": "^4.2.1",
    "styled-components": "^6.0.5",
    "typescript": "^5.1.6",
    "vite": "^4.4.5",
    "vite-plugin-checker": "^0.6.1",
    "vite-tsconfig-paths": "^4.2.0"
  }
}
